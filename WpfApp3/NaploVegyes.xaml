            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Button Content="Kiválaszt" HorizontalAlignment="Left" Margin="303,42,0,0" VerticalAlignment="Top" Width="45" Height="26" Click="Button_Click_6" RenderTransformOrigin="0.526,0.564" FontFamily="Arial Rounded MT Bold">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <TextBox x:Name="kovetelNev" HorizontalAlignment="Left" Height="26" Margin="150,42,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="ellenszamlaNevText_TextChanged"/>
        <TextBox x:Name="tartozikKod"  HorizontalAlignment="Left" Height="28" Margin="100,7,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="45" TextChanged="TextBox_TextChanged_1"/>
        <TextBox x:Name="tartozikNev" HorizontalAlignment="Left" Height="28" Margin="150,7,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <DataGrid x:Name="VegyesTetelek" HorizontalAlignment="Left" Height="142" Margin="10,237,0,0" VerticalAlignment="Top" Width="489">

            <DataGrid.Columns>
                <DataGridTextColumn x:Name="id" Header="TételSzám"/>

                <DataGridTextColumn x:Name="net" Header="Nettó"/>
                <DataGridTextColumn x:Name="afa" Header="Áfa"/>
                <DataGridTextColumn x:Name="brutto" Header="Bruttó"/>
                <DataGridTextColumn x:Name="megj" Header="Megjegyzés"/>
                <DataGridTextColumn x:Name="tartozik" Header="Tartozik számla"/>
                <DataGridTextColumn x:Name="ellensz" Header="Követel számla"/>



            </DataGrid.Columns>





















        </DataGrid>
        <Button Content="Kilépés" HorizontalAlignment="Left" Margin="707,388,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_5" FontFamily="Arial Rounded MT Bold">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Label Content="Nettó:" HorizontalAlignment="Left" Margin="552,273,0,0" VerticalAlignment="Top" Height="29"/>
        <Label Content="Áfa:" HorizontalAlignment="Left" Margin="552,307,0,0" VerticalAlignment="Top" Width="43" Height="30"/>
        <Label Content="Bruttó:" HorizontalAlignment="Left" Margin="552,342,0,0" VerticalAlignment="Top" Height="29"/>
