            <Label.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Label.Background>
        </Label>
        <Label Content="Követel" HorizontalAlignment="Left" Margin="10,42,0,0" VerticalAlignment="Top" Height="31" FontFamily="Arial Black" FontSize="14" FontWeight="Bold">
            <Label.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Label.Background>
        </Label>
        <TextBox x:Name="kovetelKod" HorizontalAlignment="Left" Height="27" Margin="99,42,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="46" TextChanged="TextBox_TextChanged_1"/>
