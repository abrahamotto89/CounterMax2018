        <TextBox x:Name="afaText" HorizontalAlignment="Left" Height="28" Margin="610,309,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="86" TextChanged="afaText_TextChanged"/>
        <TextBox x:Name="bruttoText" HorizontalAlignment="Left" Height="29" Margin="610,342,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="86" TextChanged="bruttoText_TextChanged"/>
        <ComboBox x:Name="afavalasztas" HorizontalAlignment="Left" Margin="504,309,0,0" VerticalAlignment="Top" Width="43" Height="24">
            <ComboBox.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </ComboBox.Background>
            <ComboBoxItem Content="mentes"/>
            <ComboBoxItem Content="0%"/>
            <ComboBoxItem Content="5%"/>
            <ComboBoxItem Content="18%"/>
            <ComboBoxItem Content="27%"/>

        </ComboBox>
        <Button x:Name="szamol" Content="Számol" HorizontalAlignment="Left" Margin="707,273,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_8" FontFamily="Arial Rounded MT Bold">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Button x:Name="uj" Content="Új Tétel" HorizontalAlignment="Left" Margin="10,388,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" FontFamily="Arial Rounded MT Bold">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Button x:Name="mod" Content="Módosítás" HorizontalAlignment="Left" Margin="103,388,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_11" FontFamily="Arial Rounded MT Bold">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Button x:Name="torl" Content="Törlés" HorizontalAlignment="Left" Margin="195,388,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_10" FontFamily="Arial Rounded MT Bold">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Button  x:Name="ment" Content="Mentés" HorizontalAlignment="Left" Margin="303,388,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_9" FontFamily="Arial Rounded MT Bold">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Button Content="Kiválaszt" HorizontalAlignment="Left" Margin="303,7,0,0" VerticalAlignment="Top" Width="45" Height="28" Click="Button_Click_13" FontFamily="Arial Rounded MT Bold">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Button Content="+" HorizontalAlignment="Left" Margin="275,7,0,0" VerticalAlignment="Top" Width="23" Height="26" Click="Button_Click_12">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Label Content="K Nyitó" HorizontalAlignment="Left" Margin="10,142,0,0" VerticalAlignment="Top" Width="75" FontWeight="Bold" FontFamily="Arial Black" FontSize="14">
            <Label.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Label.Background>
        </Label>
        <TextBox x:Name="KNyito" HorizontalAlignment="Left" Height="23" Margin="99,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="102"/>

    </Grid>
</Window>
