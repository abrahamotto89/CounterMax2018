        <TextBox x:Name="megjText" HorizontalAlignment="Left" Height="27" Margin="430,201,0,0" TextWrapping="Wrap" Text=" " VerticalAlignment="Top" Width="250" TextChanged="TextBox_TextChanged_2"/>
        <Button Content="Kiválaszt" HorizontalAlignment="Left" Margin="727,125,0,0" VerticalAlignment="Top" Width="55" RenderTransformOrigin="1.364,0.545" Click="Button_Click_1" Height="26" FontFamily="Arial Rounded MT Bold">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Button Content="Kiválaszt" HorizontalAlignment="Left" Margin="727,164,0,0" VerticalAlignment="Top" Width="55" Click="Button_Click_3" Height="26" FontFamily="Arial Rounded MT Bold">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <TextBox x:Name="munkaNev" HorizontalAlignment="Left" Height="26" Margin="485,128,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="195" TextChanged="TextBox_TextChanged_2"/>
        <TextBox x:Name="partnerNev" HorizontalAlignment="Left" Height="27" Margin="485,164,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="195" TextChanged="TextBox_TextChanged_3"/>
        <Button Content="+" HorizontalAlignment="Left" Margin="697,125,0,0" VerticalAlignment="Top" Width="25" Click="Button_Click_2" Height="26">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Button Content="+" HorizontalAlignment="Left" Margin="697,164,0,0" VerticalAlignment="Top" Width="25" Click="Button_Click_4" Height="26">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Button.Background>
        </Button>
        <Label Content="TNyitó" HorizontalAlignment="Left" Margin="10,111,0,0" VerticalAlignment="Top" FontSize="14" FontFamily="Arial Black" Width="75">
            <Label.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Label.Background>
        </Label>
        <TextBox x:Name="TNyito" HorizontalAlignment="Left" Height="26" Margin="99,111,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="102" TextChanged="nyito_TextChanged"/>
        <Label Content="Sorszám" HorizontalAlignment="Left" Margin="589,7,0,0" VerticalAlignment="Top" Width="68" FontFamily="Arial Rounded MT Bold"/>
        <TextBox x:Name="sorszamText" HorizontalAlignment="Left" Height="23" Margin="662,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Kelt:" HorizontalAlignment="Left" Height="24" Margin="10,82,0,0" VerticalAlignment="Top" Width="90" FontSize="14" FontFamily="Arial Black">
            <Label.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Label.Background>
        </Label>
        <DatePicker x:Name="keltText" HorizontalAlignment="Left" Margin="99,82,0,0" VerticalAlignment="Top" Height="24"/>
