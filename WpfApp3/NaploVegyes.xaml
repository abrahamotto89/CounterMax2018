        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title="Vegyes Napló" Height="450" Width="800" ResizeMode="NoResize">
    <Grid>
        <Grid.Background>
            <SolidColorBrush Color="{DynamicResource 12}"/>
        </Grid.Background>
        <Label Content="Bizonylatszám:" HorizontalAlignment="Left" Height="31" Margin="303,91,0,0" VerticalAlignment="Top" Width="120" FontFamily="Arial Black" FontSize="14">
            <Label.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Label.Background>
        </Label>
        <Label Content="Munkaszám" HorizontalAlignment="Left" Margin="303,128,0,0" VerticalAlignment="Top" Width="120" Height="30" FontSize="14" FontFamily="Arial Black">
            <Label.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Label.Background>
        </Label>
        <Label Content="Partner:" HorizontalAlignment="Left" Margin="303,160,0,0" VerticalAlignment="Top" Width="120" Height="31" FontFamily="Arial Black" FontSize="14">
            <Label.Background>
                <SolidColorBrush Color="{DynamicResource 12}"/>
            </Label.Background>
        </Label>
        <Label Content="Megjegyzés:" HorizontalAlignment="Left" Margin="303,197,0,0" VerticalAlignment="Top" Width="120" Height="31" FontSize="14" FontFamily="Arial Black">
